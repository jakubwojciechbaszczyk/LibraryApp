<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_persons">

    <fragment
        android:id="@+id/navigation_rents"
        android:name="com.example.libraryapp.ui.rent.RentsFragment"
        android:label="@string/title_rents"
        tools:layout="@layout/fragment_persons" />

    <fragment
        android:id="@+id/navigation_positions"
        android:name="com.example.libraryapp.ui.position.PositionsFragment"
        android:label="@string/title_persons"
        tools:layout="@layout/fragment_positions" />

    <fragment
        android:id="@+id/navigation_persons"
        android:name="com.example.libraryapp.ui.person.PersonsFragment"
        android:label="@string/title_positions"
        tools:layout="@layout/fragment_rents" />
